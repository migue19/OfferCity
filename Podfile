# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'OfferCity' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for OfferCity

   pod 'Firebase/Core'
   pod 'Firebase/Auth'
   pod 'FBSDKCoreKit'
   pod 'FBSDKShareKit'
   pod 'FBSDKLoginKit'
   pod 'GoogleSignIn'
   pod 'GoogleMaps'
   pod 'GooglePlaces'
   pod 'AlamofireSwiftyJSON'
   pod 'HandyJSON'
   pod 'CarbonKit'
end
